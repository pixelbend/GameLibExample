<?xml version="1.0" encoding="UTF-8"?>
<logic>

    <tweenTestTitleLayout x="0" textWidth="0.2" textHeight="0.15" />

    <tweenTestButtonLayout buttonWidth="0.1" buttonHeight="0.15">
        <button x=".25" buttonID="play" linkage="buttonPlayLinkage"/>
        <button x=".38" buttonID="pause" linkage="buttonPauseLinkage"/>
        <button x=".51" buttonID="stop" linkage="buttonStopLinkage"/>
        <button x=".64" buttonID="reset" linkage="buttonResetLinkage"/>
    </tweenTestButtonLayout>

    <tweenTestSetups>
        <tweenTestSetup y=".37" properties="x:0.75;y:0.7" duration="2000" tweenNameTextID="move" dependentOnScreenSize="true" />
        <tweenTestSetup y=".57" properties="scaleX:1.3;scaleY:0.4" duration="2000" tweenNameTextID="scale" />
    </tweenTestSetups>
</logic>